<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="svg_canvas" />
		<Option execution_dir="./" />
		<Option pch_mode="2" />
		<Option default_target="win_gcc" />
		<Option compiler="null" />
		<Build>
			<Target title="win_gcc">
				<Option output="bin/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-g" />
					<Add option="`$(#WX_CONFIG) --cflags`" />
					<Add directory="liblunasvg/include/lunasvg" />
				</Compiler>
				<ResourceCompiler>
					<Add option="`$(#WX_CONFIG) --rcflags`" />
				</ResourceCompiler>
				<Linker>
					<Add option="`$(#WX_CONFIG) --libs base,core`" />
					<Add library="liblunasvg/bin/liblunasvg.dll" />
					<Add directory="liblunasvg/bin" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="win_gcc;" />
		</VirtualTargets>
		<ResourceCompiler>
			<Add directory="%WXWIN%/include" />
		</ResourceCompiler>
		<Unit filename="main.cpp" />
		<Unit filename="sample.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="svg_canvas.cpp" />
		<Unit filename="svg_canvas.h" />
		<Unit filename="svg_image_luna.cpp" />
		<Unit filename="svg_image_luna.h" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
